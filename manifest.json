{
    "manifest_version": 3,
    "name": "Multiwallet2",
    "version": "1.0.0",
    "action": {
        "default_title": "Click to open"
    },
    "background": {
        "service_worker": "src/background/background.ts",
        "type": "module"
    },
    "content_scripts": [
        {
            "js": [
                "src/content/content.ts"
            ],
            "matches": [
                "*://*/*"
            ]
        }
    ],
    "side_panel": {
        "default_path": "index.html"
    },
    "permissions": [
        "sidePanel",
        "storage"
    ]
}